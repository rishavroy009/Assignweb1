<!DOCTYPE html>
<html>
<head>
    <title>ClearKey DRM Test</title>
    <script src="https://cdn.dashjs.org/latest/dash.all.min.js"></script>
</head>
<body>
    <h1>ClearKey DRM Encrypted Video</h1>
    <video id="videoPlayer" controls width="640" height="360"></video>

    <script>
        var url = 'http://localhost/out.mpd';
        var player = dashjs.MediaPlayer().create();
        player.initialize(document.querySelector("#videoPlayer"), url, true);

        // Set up ClearKey protection
        player.setProtectionData({
            "org.w3.clearkey": {
                "clearkeys": {
                    "f239e769f45bb2c1b5e5f22b014fed8c": "1e4b78991234abcd56781234def56789"
                }
            }
        });
    </script>
</body>
</html>
