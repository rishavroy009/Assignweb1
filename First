ffmpeg -i input.mp4 -c:v copy -c:a copy -f mp4 \
-encryption_scheme cenc-aes-ctr \
-encryption_key abcdef1234567890abcdef1234567890 \
-encryption_kid 1234567890abcdef1234567890abcdef \
output_encrypted.mp4


MP4Box -dash 4000 -rap -profile onDemand -out output.mpd \
-encryption-key=1234567890abcdef1234567890abcdef:abcdef1234567890abcdef1234567890 \
output_encrypted.mp4


sudo mv output.mpd /usr/local/var/www/
sudo mv output_encrypted*.m4s /usr/local/var/www/
