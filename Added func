// Select all <p> elements
const pTags = document.querySelectorAll('p');

// Iterate through each <p> tag
pTags.forEach((pTag) => {
    // Check if the font family is set to 'default'
    if (pTag.style.fontFamily === 'default') {
        // Set genericFamilyName to proportionalSansSerif
        pTag.style.fontFamily = 'proportionalSansSerif';
    }
});
